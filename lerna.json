{
  "packages": [
    "./ui/*",
    "./ssr-testing"
  ],
  "useWorkspaces": true,
  "npmClient": "npm",
  "command": {
    "version": {
      "exact": true,
      "conventionalCommits": true,
      "message": "chore(release): publish"
    },
    "publish": {
      "conventionalCommits": true,
      "message": "chore(release): publish",
      "npmClient": "npm",
      "ignoreChanges": [
        "**/stories/**",
        "**/tests/**"
      ],
      "registry": "https://registry.npmjs.org/",
      "allowBranch": "main"
    }
  },
  "version": "0.3.0-experimental.1"
}
