{
	"name": "@washingtonpost/ui-autosuggest",
	"version": "0.1.0-experimental.0",
	"description": "> TODO: description",
	"author": "artmsilva <artmsilva@gmail.com>",
	"homepage": "https://github.com/WPMedia/wpds-ui-kit#readme",
	"license": "MIT",
	"source": "src/index.js",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"exports": {
		".": {
			"require": "./dist/cjs/index.js",
			"default": "./dist/esm/index.js"
		}
	},
	"files": [
		"dist"
	],
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/WPMedia/wpds-ui-kit.git"
	},
	"scripts": {
		"test": "echo \"Error: run tests from root\" && exit 1",
		"build:esm": "BABEL_ENV=esm babel src --root-mode upward --extensions .js -d dist/esm --source-maps",
		"build:cjs": "BABEL_ENV=cjs babel src --root-mode upward --extensions .js -d dist/cjs --source-maps",
		"dev:cjs": "BABEL_ENV=cjs babel --watch src --root-mode upward --extensions .js -d dist/cjs --source-maps",
		"dev:esm": "BABEL_ENV=esm babel --watch src --root-mode upward --extensions .js -d dist/esm --source-maps",
		"size": "size-limit",
		"size:why": "size-limit --why",
		"build": "concurrently npm:build:*",
		"dev": "concurrently npm:dev:*"
	},
	"bugs": {
		"url": "https://github.com/WPMedia/wpds-ui-kit/issues"
	},
	"devDependencies": {
		"react": "^16.14.0"
	},
	"peerDependencies": {
		"react": ">=16.8.6"
	},
	"dependencies": {
		"@washingtonpost/ui-theme": "file:../theme"
	},
	"size-limit": [
		{
			"limit": "1 kb",
			"path": "dist/esm/autosuggest.js"
		}
	]
}
